{
  "name": "gateway-test",
  "version": "1.0.0",
  "description": "gateway-test",
  "main": "index.js",
  "scripts": {
    "install-dnsmasq": "bash scripts/install-dnsmasq.sh",
    "dev": "ts-node-dev --respawn --transpileOnly ./src/gateway/app.ts",
    "terraform": "cd terraform/environments/dev && terraform apply",
    "tsc": "tsc",
    "build": "docker build -t gateway .",
    "build-run": "docker run --env-file=.env --env AWS_SECRET_ACCESS_KEY=$(echo $AWS_SECRET_ACCESS_KEY) --env AWS_ACCESS_KEY_ID=$(echo $AWS_ACCESS_KEY_ID) --env AWS_REGION=$(echo $AWS_REGION) --env AWS_SESSION_TOKEN=$(echo $AWS_SESSION_TOKEN) -it -p 3000:3000 gateway",
    "build-tag": "docker tag gateway:latest 384386061638.dkr.ecr.eu-west-2.amazonaws.com/arweave-gateway:latest",
    "build-push": "docker push 384386061638.dkr.ecr.eu-west-2.amazonaws.com/arweave-gateway:latest",
    "ecs-login": "aws ecr get-login --no-include-email --region eu-west-2 | /bin/bash",
    "local-cert": "openssl req -config tls/arweave.localhost/request.cnf -new -nodes -x509 -newkey rsa:2048 -sha256 -keyout tls/arweave.localhost/key.pem -out tls/arweave.localhost/cert.pem -days 365"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arweave-kyle/gateway-test.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/arweave-kyle/gateway-test/issues"
  },
  "homepage": "https://github.com/arweave-kyle/gateway-test#readme",
  "dependencies": {
    "abort-controller": "^3.0.0",
    "apollo-server-express": "^2.13.1",
    "async-retry": "^1.3.1",
    "aws-sdk": "^2.682.0",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-async-handler": "^1.1.4",
    "express-promise-router": "^3.0.3",
    "graphql": "^14.6.0",
    "helmet": "^3.22.0",
    "http-errors": "^1.7.3",
    "knex": "^0.20.15",
    "lodash": "^4.17.15",
    "moment": "^2.26.0",
    "morgan": "^1.10.0",
    "node-fetch": "^2.6.0",
    "pg": "^7.18.2",
    "rfc4648": "^1.3.0"
  },
  "devDependencies": {
    "@types/async-retry": "^1.4.2",
    "@types/aws-lambda": "^8.10.51",
    "@types/aws-serverless-express": "^3.3.3",
    "@types/express": "^4.17.6",
    "@types/helmet": "0.0.47",
    "@types/http-errors": "^1.6.3",
    "@types/lodash": "^4.14.152",
    "@types/morgan": "^1.9.0",
    "@types/node-fetch": "^2.5.7",
    "@types/pg": "^7.14.3",
    "serverless-deployment-bucket": "^1.1.2",
    "serverless-export-env": "^1.3.1",
    "serverless-offline": "^6.1.7",
    "serverless-offline-sqs": "^3.1.3",
    "serverless-stage-manager": "^1.0.5",
    "ts-node": "^8.10.1",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.9.3"
  }
}
