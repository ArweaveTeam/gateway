{
  "name": "gateway-test",
  "version": "1.0.0",
  "description": "gateway-test",
  "main": "index.js",
  "scripts": {
    "install-dnsmasq": "bash scripts/install-dnsmasq.sh",
    "start": "export IS_LOCAL=1 && tsc && serverless offline --httpPort 3000 --host arweave.localhost --noPrependStageInUrl --httpsProtocol tls/arweave.localhost",
    "terraform": "cd terraform/environments/dev && terraform apply",
    "init-regional-gateway": "serverless create_domain",
    "deploy": "tsc && serverless deploy",
    "info": "serverless info",
    "local-cert": "openssl req -config tls/arweave.localhost/request.cnf -new -nodes -x509 -newkey rsa:2048 -sha256 -keyout tls/arweave.localhost/key.pem -out tls/arweave.localhost/cert.pem -days 365"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arweave-kyle/gateway-test.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/arweave-kyle/gateway-test/issues"
  },
  "homepage": "https://github.com/arweave-kyle/gateway-test#readme",
  "dependencies": {
    "abort-controller": "^3.0.0",
    "arweave": "^1.6.0",
    "knex": "^0.20.13",
    "lambda-api": "^0.10.5",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "node-fetch": "^2.6.0",
    "pg": "^7.18.2",
    "rfc4648": "^1.3.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.48",
    "@types/aws-serverless-express": "^3.3.3",
    "@types/express": "^4.17.5",
    "@types/node-fetch": "^2.5.5",
    "@types/pg": "^7.14.3",
    "dotenv": "^8.2.0",
    "serverless-api-gateway-caching": "^1.4.1",
    "serverless-deployment-bucket": "^1.1.1",
    "serverless-domain-manager": "^3.3.1",
    "serverless-offline": "^6.1.4",
    "serverless-offline-sqs": "^3.1.3",
    "serverless-stage-manager": "^1.0.5",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3"
  }
}
